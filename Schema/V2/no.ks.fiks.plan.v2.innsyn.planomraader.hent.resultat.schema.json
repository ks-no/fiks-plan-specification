{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "no.ks.fiks.plan.v2.innsyn.planomraader.resultat.schema.json",
    "title": "HentPlanomraaderResultat",
    "definitions": {},
    "type": "object",
    "properties": {
        "planomraader": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "omraade": {
                        "type": "object",
                        "properties": {
                            "koordinatsystem": {
                                "type": "string"
                            },
                            "ytreAvgrensning": {
                                "type": "string"
                            },
                            "vertikallag": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "koordinatsystem",
                            "ytreAvgrensning"
                        ]
                    },
                    "vertikalnivaa": {
                        "type": "string",
                        "enum": [
                            "Under grunnen",
                            "På grunnen/vannoverflate",
                            "Over grunnen (bru)",
                            "På bunnen (vann/sjoe)",
                            "I vannsoeylen"
                        ]
                    },
                    "vertikallag": {
                        "type": "object",
                        "properties": {
                            "lag": {
                                "type": "string"
                            },
                            "referansehoeyde": {
                                "type": "object",
                                "properties": {
                                    "hoeydeverdi": {
                                        "type": "string"
                                    },
                                    "hoeydereferansesystem": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}