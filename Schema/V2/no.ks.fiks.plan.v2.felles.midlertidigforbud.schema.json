                    "type": "string"
                },
                "kodebeskrivelse": {
                    "type": "string"
                }
            }
        },
        "gyldigTilDato": {
            "type": "string",
            "format": "date"
        },
        "omraadenavn": {
            "type": "string"
        }
    },
    "required": [
        "omraade",
        "avgjoerelsedato",
        "pblTiltakForbudtype",
        "gyldigTilDato"
    ]
