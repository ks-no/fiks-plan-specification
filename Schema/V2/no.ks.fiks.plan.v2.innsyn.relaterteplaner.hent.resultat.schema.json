{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "no.ks.fiks.plan.v2.innsyn.relaterteplaner.hent.resultat.schema.json",
    "title": "HentRelatertePlanerResultat",
    "definitions": {},
    "type": "object",
    "properties": {
      "nasjonalArealplanId": {
        "$ref": "no.ks.fiks.plan.v2.felles.nasjonalarealplanid.schema.json"
      },
      "planrelasjon": {
        "type": "array",
        "items": {
          "properties": {
            "planforhold": {
              "type": "object",
              "properties": {
                "kodeverdi": {
                  "type": "string"
                },
                "kodebeskrivelse": {
                  "type": "string"
                }
              },
              "required": [
                "kodeverdi",
                "kodebeskrivelse"
              ]
            },
            "arealplan": {
              "type": "object",
              "properties": {
                "nasjonalArealplanId": {
                  "$ref": "no.ks.fiks.plan.v2.felles.nasjonalarealplanid.schema.json"
                },
                "plantype": {
                  "type": "object",
                  "properties": {
                    "kodeverdi": {
                      "type": "string"
                    },
                    "kodebeskrivelse": {
                      "type": "string"
                    }
                  }
                },
                "plannavn": {
                  "type": "string"
                },
                "planstatus": {
                  "type": "object",
                  "properties": {
                    "kodeverdi": {
                      "type": "string"
                    },
                    "kodebeskrivelse": {
                      "type": "string"
                    }
                  }
                },
                "plandokumentasjonOppdatert": {
                  "type": "boolean"
                },
                "ubehandletKlage": {
                  "type": "boolean"
                },
                "ubehandletInnsigelse": {
                  "type": "boolean"
                },
                "vedtakEndeligPlanDato": {
                  "type": "string",
                  "format": "date"
                },
                "lovreferanse": {
                  "type": "object",
                  "properties": {
                    "kodeverdi": {
                      "type": "string"
                    },
                    "kodebeskrivelse": {
                      "type": "string"
                    }
                  }
                },
                "forslagstillerType": {
                  "type": "string",
                  "enum": [
                    "offentlig",
                    "privat"
                  ]
                }
              },
              "required": [
                "nasjonalArealplanId",
                "plantype",
                "plannavn",
                "planstatus",
                "plandokumentasjonOppdatert",
                "ubehandletKlage",
                "ubehandletInnsigelse",
                "forslagstillerType"
              ]
            }
          }
        },
        "required": [
          "planforhold",
          "arealplan"
        ]
      }
    },
    "required": [
      "nasjonalArealplanId"
    ]
  }