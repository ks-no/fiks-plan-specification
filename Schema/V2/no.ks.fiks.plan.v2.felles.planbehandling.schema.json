{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "no.ks.fiks.plan.v2.felles.planbehandling.schema.json",
    "title": "Planbehandling",
    "definitions": {},
    "type": "object",
    "properties": {
        "posisjon": {
            "$ref": "no.ks.fiks.plan.v2.felles.posisjon.schema.json"
        },
        "dato": {
            "type": "string",
            "format": "date"
        },
        "saksnummer": {
            "$ref": "no.ks.fiks.plan.v2.felles.saksnummer.schema.json"
        },
        "planbehandlingtype": {
            "type": "object",
            "properties": {
                "kodeverdi": {
                    "type": "string"
                },
                "kodebeskrivelse": {
                    "type": "string"
                }
            }
        },
        "navn": {
            "type": "string"
        },
        "plandokumenter": {
            "type": "array",
            "items": {
                "$ref": "no.ks.fiks.plan.v2.felles.dokument.schema.json"
            }
        }
    },
    "required": [
        "dato",
        "saksnummer",
        "planbehandlingtype",
        "navn"
    ]
}